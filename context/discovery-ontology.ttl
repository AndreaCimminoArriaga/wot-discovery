			dc:creator "Andrea Cimmino" ;
			dc:creator "Farshid Tavakolizadeh" ;
			owl:versionInfo "0.1.1" ;
			dc:title "WoT discovery ontology" ;
			dc:description "Ontology code created by Chowlk" .

